Xcursor.theme: Maia-Cursor

! font settings --------------------------------------------------------------

Xft.autohint:               true
Xft.antialias:              true
Xft.hinting:                true
Xft.hintstyle:              hintslight
Xft.rgba:                   rgb
Xft.lcdfilter:              lcddefault


! URXVT FONTS
!------------------------------------------------
URxvt*font:                 xft:HackGen:style=Regular:pixelsize=14, xft:Noto Sans CJK KR:style=Regular:pixelsize=14, xft:Noto Sans CJK SC:style=Regular:pixelsize=14, xft:Noto Color Emoji:pixelsize=14
URxvt*boldFont:             xft:HackGen:style=Regular:pixelsize=14, xft:Noto Sans CJK KR:style=Regular:pixelsize=14, xft:Noto Sans CJK SC:style=Regular:pixelsize=14, xft:Noto Color Emoji:pixelsize=14
URxvt*letterSpace:          -7.3
URxvt*allow_bold:           true


! URXVT ENABLE LINK SUPPORT
!------------------------------------------------

URxvt.perl-ext-common:      default,matcher,clipboard
!tabbed
URxvt.matcher.button:   1
URxvt.url-select.launcher:  /usr/bin/chromium
URxvt.url-select.underline: true

!disable the fucking belxl
URxvt.insecure:			    false

! URXVT SCROLLBAR AND CURSOR STYLE
!------------------------------------------------

URxvt*saveLines:		    300000
URxvt.scrollBar: 		    false
!URxvt*scrollstyle:		    plain
URxvt*cursorBlink: 		    true
URxvt*cursorUnderline: 	    true
URxvt*internalBorder:       25

! URXVT FULLSCREEN TOGGLE
!------------------------------------------------

URxvt.keysym.F11: 			perl:fullscreen:switch

